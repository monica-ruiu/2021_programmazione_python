n = int(input("Inserisci un numero"))
            
lista_pari = []
for i in range (0,n+1) :
    if is_pari(i):
         lista_pari.append(i)

contatore = 0
for numero_pari in lista_pari:
    contatore = contatore +1
    print("Elemento in posisizone" + str(contatore-1) + ":" + str(numero_pari))
